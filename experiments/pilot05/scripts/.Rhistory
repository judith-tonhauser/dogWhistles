means = d %>%
group_by(item) %>%
summarize(Mean = mean(rating), CILow = ci.low(rating), CIHigh = ci.high(rating)) %>%
ungroup() %>%
mutate(YMin = Mean - CILow, YMax = Mean + CIHigh)
means
